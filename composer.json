            "Muscobytes\\Poizon\\DistributionApiClient\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "psr/http-client-implementation": "*",
        "psr/http-factory-implementation": "*",
        "php-http/discovery": "^1.20",
        "psr/http-client": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
