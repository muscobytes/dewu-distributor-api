{
    "name": "muscobytes/poizon-distribution-api-client",
    "description": "",
    "version": "0.0.1",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^2.1",
        "nyholm/psr7": "^1.8",
        "symfony/http-client": "^6.4",
        "psy/psysh": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Muscobytes\\Poizon\\DistributionApiClient\\": "src/"
        }
